<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="563.0" prefWidth="932.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="student.studentController">
   <children>
      <TabPane prefHeight="563.0" prefWidth="932.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Student Data">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <TableView fx:id="studentTable" editable="true" prefHeight="526.0" prefWidth="932.0" tableMenuButtonVisible="true">
